pipeline {

  agent { label 'kubepod' }

  stages {

    stage('Checkout Source') {
      steps {
        git url:'https://github.com/lingamnagani/javademo.git', branch:'master'
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "conf.yaml", kubeconfigId: "mykubeconfig")
        }
      }
    }

  }

}
